{
	"extends": [
		"eslint:recommended",
		"plugin:node/recommended",
		"airbnb",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"plugins": [
		"react",
		"prettier"
	],
	"rules": {
		"prettier/prettier":"error",
		"no-unused-vars":"warn",
		"func-names":"off",
		"no-underscore-dangle": ["error", { "allow": ["_id"] }]
	}
}

